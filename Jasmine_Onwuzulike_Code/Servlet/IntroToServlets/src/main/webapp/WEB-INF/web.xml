<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>IntroToServlets</display-name>
  <servlet>
  <servlet-name>hello</servlet-name>
  <servlet-class>com.revature.HelloWorldServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>hello</servlet-name>
  <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>fbr</servlet-name>
  <servlet-class>com.revature.ForwardvsRedirectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>fbr</servlet-name>
  <url-pattern>/fbr</url-pattern>
  </servlet-mapping>
  
</web-app>