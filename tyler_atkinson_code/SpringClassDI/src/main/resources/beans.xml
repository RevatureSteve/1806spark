<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id ="fastCar" class="com.revature.model.Car">
	<constructor-arg name = "name" value = "Fast Car"> </constructor-arg>
	<property name = "color" value ="black"></property>
	<property name = "engine" ref="powerfulEngine"></property>
</bean>

<bean id = "powerfulEngine" class="com.revature.model.Engine"></bean>

<bean id = "employeeService" class="com.revature.service.EmployeeService"></bean>
	

<bean id ="employeeDaoImpl" class= "com.revature.dao.EmployeeDaoImpl"></bean>


</beans>
