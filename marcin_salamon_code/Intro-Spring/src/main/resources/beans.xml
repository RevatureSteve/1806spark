<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="fastCar" class="com.revature.bean.Car">
		<constructor-arg name="name" value="Fast Car"></constructor-arg>
		<property name="color" value="black"></property>
		<property name="engine" value="powerfulEngine"></property>
	</bean>

<bean id="powerfunEngine" class="com.revature.bean.Engine">
	
</bean>
</beans>
