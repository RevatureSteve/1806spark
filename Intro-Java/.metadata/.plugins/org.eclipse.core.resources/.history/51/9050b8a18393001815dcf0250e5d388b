package com.project.objs;

import com.project.Interface;

public class Knife extends Cutter{



	public Knife() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Knife(String name, int degreeSharpness) {
		super(name, degreeSharpness);
		name = "Knife";
		degreeSharpness = 0;
	}

	public void Slice(){
		System.out.println( "You slice the bread.");
		
		}
		// TODO Auto-generated method stub

	@Override
	public String toString() {
		return "[getName()=" + getName() + ", getDegreeSharpness()=" + getDegreeSharpness()"]";
	}
	
	
	}

	


