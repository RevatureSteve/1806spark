	public Transport() {};
	
	public Transport(int passengerCount) {
		this.passengerCount = passengerCount;
	};
	
	
	
	@Override
	public void move() {
		System.out.println("Transport rumbles along.");
		// TODO Auto-generated method stub
		
	}

}


}
