<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>PokemonWorldAPI</display-name>
  <servlet>
    <description></description>
    <display-name>CreateWorldServlet</display-name>
    <servlet-name>CreateWorldServlet</servlet-name>
    <servlet-class>com.cpo.servlets.CreateWorldServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateWorldServlet</servlet-name>
    <url-pattern>/CreateWorld</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PokemonDataServlet</display-name>
    <servlet-name>PokemonDataServlet</servlet-name>
    <servlet-class>com.cpo.servlets.PokemonDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PokemonDataServlet</servlet-name>
    <url-pattern>/PokemonData</url-pattern>
  </servlet-mapping>
</web-app>