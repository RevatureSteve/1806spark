SELECT * FROM users;
SELECT * FROM reimbursement;

SELECT * FROM
(SELECT REIMBURSEMENT.r_id,REIMBURSEMENT.RQ_TYPE_ID,REIMBURSEMENT.R_SUBMISSION_UID,REIMBURSEMENT.R_RESOLVED__UID,
REIMBURSEMENT.RQ_STATUS_ID,REIMBURSEMENT.AMOUNT,REIMBURSEMENT.DESCRIPTION,REIMBURSEMENT.IMG,REIMBURSEMENT.TIME_SUBMISSION,
REQUEST_STATUS.RQ_STATUS_ID,REQUEST_STATUS.RQ_STATUS
FROM reimbursement INNER JOIN request_status ON reimbursement.rq_status_id = request_status.rq_status_id)
WHERE rq_status= 'Pending';



SELECT * FROM position WHERE pos_id= 1;


COMMIT;


SELECT r_id,rq_type,fname,R_RESOLVED__UID,rq_status,amount,description,img,time_submission FROM
(SELECT REIMBURSEMENT.*,REQUEST_STATUS.*,Request_type.*,users.*
FROM reimbursement INNER JOIN request_status ON reimbursement.rq_status_id = request_status.rq_status_id
INNER JOIN request_type ON reimbursement.rq_type_id=request_type.rq_type_id
INNER JOIN USERS ON reimbursement.R_SUBMISSION_UID = users.u_id)
WHERE rq_status= 'Pending';

SELECT r_id,rq_type,fname,lname,R_RESOLVED__UID,rq_status,amount,description,img,time_submission FROM
(SELECT REIMBURSEMENT.*,REQUEST_STATUS.*,Request_type.*,users.*
FROM reimbursement INNER JOIN request_status ON reimbursement.rq_status_id = request_status.rq_status_id
INNER JOIN request_type ON reimbursement.rq_type_id=request_type.rq_type_id
INNER JOIN USERS ON reimbursement.R_SUBMISSION_UID = users.u_id)
WHERE u_id= 138;

UPDATE users
SET email = 'humphreyminott@gmail'
where u_id = 138;

commit;

