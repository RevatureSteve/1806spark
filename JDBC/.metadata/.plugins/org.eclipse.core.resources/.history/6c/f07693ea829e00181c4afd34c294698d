public class Login {

	public static Login validateUser() {

//		String emailInput = null;
//		String pwInput = "nana";
		String fname = null;
		String lname = null;

		Scanner scan = new Scanner(System.in);

		System.out.println("Enter Username: ");
		String emailInput = scan.nextLine();
		System.out.println("Enter Password: ");
		String pwInput = scan.nextLine();
		User validUser = new AppService().login(new User(0, emailInput, pwInput, fname, lname, 0));

		return this;
	}

}
