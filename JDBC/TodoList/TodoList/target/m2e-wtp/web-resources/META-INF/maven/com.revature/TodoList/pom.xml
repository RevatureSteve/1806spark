  <groupId>com.revature</groupId>
  <artifactId>TodoList</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>war</packaging>
  
  <!--  no java version #? what is the default for maven? 
  			Maven defaults to....1.5 so update it!
  -->
  <properties>
   	<maven.compiler.target>1.8</maven.compiler.target>
   	<maven.compiler.source>1.8</maven.compiler.source>
  </properties>
  
 <dependencies>
  <!-- I can list multiple dependencies (JARS) i here
  			Once I place a dependency, Maven will download that JAR
  			 from Maven Central  (the website)
  			 into a local folder called .m2, reguardless
  			 of the type of OS -->
<!-- https://mvnrepository.com/artifact/javax.servlet/servlet-api -->
<dependency>
    <groupId>javax.servlet</groupId>
    <artifactId>servlet-api</artifactId>
    <version>2.5</version>
    <scope>provided</scope>
</dependency>

</dependencies>
</project>