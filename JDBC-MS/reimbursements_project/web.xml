<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>reimbursement</display-name>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>EmployeeReimbursements</display-name>
		<servlet-name>EmployeeReimbursements</servlet-name>
		<servlet-class>com.revature.servlets.EmployeeReimbursementsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeReimbursements</servlet-name>
		<url-pattern>/employee/reimbursements</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>EmployeeReimbursementsPending</display-name>
		<servlet-name>EmployeeReimbursementsPending</servlet-name>
		<servlet-class>com.revature.servlets.EmployeeReimbursementsPendingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeReimbursementsPending</servlet-name>
		<url-pattern>/employee/reimbursements/pending</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>EmployeeReimbursementsResolved</display-name>
		<servlet-name>EmployeeReimbursementsResolved</servlet-name>
		<servlet-class>com.revature.servlets.EmployeeReimbursementsResolvedServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeReimbursementsResolved</servlet-name>
		<url-pattern>/employee/reimbursements/resolved</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UserInformationUpdateServlet</display-name>
		<servlet-name>UserInformationUpdateServlet</servlet-name>
		<servlet-class>com.revature.servlets.UserInformationUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserInformationUpdateServlet</servlet-name>
		<url-pattern>/employee/update</url-pattern>
		<url-pattern>/manager/update</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>SubmitReimbursementServlet</display-name>
		<servlet-name>SubmitReimbursementServlet</servlet-name>
		<servlet-class>com.revature.servlets.SubmitReimbursementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SubmitReimbursementServlet</servlet-name>
		<url-pattern>/employee/reimbursements/submit</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ManagerReimbursementServlet</display-name>
		<servlet-name>ManagerReimbursementServlet</servlet-name>
		<servlet-class>com.revature.servlets.ManagerReimbursementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ManagerReimbursementServlet</servlet-name>
		<url-pattern>/manager</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>CorsFilter</filter-name>
		<filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
		<init-param>
			<param-name>cors.allowed.origins</param-name>
			<param-value>*</param-value>
		</init-param>
		<init-param>
			<param-name>cors.allowed.methods</param-name>
			<param-value>GET,POST,HEAD,OPTIONS,PUT</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>CorsFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>