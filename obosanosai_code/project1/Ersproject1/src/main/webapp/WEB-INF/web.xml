    <servlet-name>ResolvedReimbursementServlet</servlet-name>
    <servlet-class>com.revature.servlets.ResolvedReimbursementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResolvedReimbursementServlet</servlet-name>
    <url-pattern>/resolved</url-pattern>
  </servlet-mapping>
  <servlet>
