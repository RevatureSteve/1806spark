    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PendingReimbursementIdServlet</display-name>
    <servlet-name>PendingReimbursementIdServlet</servlet-name>
    <servlet-class>com.revature.servlets.PendingReimbursementIdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PendingReimbursementIdServlet</servlet-name>
